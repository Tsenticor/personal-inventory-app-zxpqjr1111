{
  "expo": {
    "name": "Inventory",
    "slug": "inventory",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/inventory-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_inventory-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.inventory",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Для выбора фото предметов из галереи",
        "NSCameraUsageDescription": "Для фотографирования предметов с помощью камеры",
        "NSPhotoLibraryAddUsageDescription": "Для сохранения фото предметов в галерею"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/inventory-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.inventory",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "blockedPermissions": []
    },
    "web": {
      "favicon": "./assets/images/inventory-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "Разрешите доступ к галерее для выбора фото предметов",
          "cameraPermission": "Разрешите доступ к камере для съемки фото предметов"
        }
      ]
    ],
    "scheme": "inventory",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f2958680-909f-428a-b06e-b6879704b6e4"
      }
    },
    "owner": ""
  }
}